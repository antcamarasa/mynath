<div class="container">
  <div>
    <h1>Mes ordonnances</h1>
  </div>
  <div>
      <% @prescriptions.reverse_each do |prescription| %>
        <% if prescription.user_id == current_user.id %>
        <h2><strong><%= prescription.description %></strong></h2>
        <p><%= prescription.date %></p>
        <% end %>
      <% end %>
  </div>
</div>
